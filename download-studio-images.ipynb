    "# Classe completa do downloader\n",
    "class StudioImageDownloader:\n",
    "    def __init__(self, dry_run: bool = False):\n",
    "        self.dry_run = dry_run\n",
    "        self.supabase: Client = create_client(SUPABASE_URL, SUPABASE_KEY)\n",
    "        self.session = requests.Session()\n",
    "        self.session.headers.update({\n",
    "            'User-Agent': 'Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36'\n",
    "        })\n",
    "        \n",
    "        self.stats = {'total': 0, 'downloaded': 0, 'skipped': 0, 'errors': 0}\n",
    "        \n",
    "        # Criar diret√≥rios\n",
    "        if not dry_run:\n",
    "            UPLOADS_DIR.mkdir(parents=True, exist_ok=True)\n",
    "            (UPLOADS_DIR / 'thumbnails').mkdir(exist_ok=True)\n",
    "            (UPLOADS_DIR / 'medium').mkdir(exist_ok=True)\n",
    "            (UPLOADS_DIR / 'original').mkdir(exist_ok=True)\n",
    "            print(f\"üìÅ Diret√≥rios criados em: {UPLOADS_DIR}\")\n",
    "\n",
    "    def normalize_string(self, text: str) -> str:\n",
    "        if not text: return ''\n",
    "        text = unicodedata.normalize('NFD', text)\n",
    "        text = ''.join(c for c in text if unicodedata.category(c) != 'Mn')\n",
    "        text = text.lower()\n",
    "        text = re.sub(r'[^a-z0-9\\s-]', '', text)\n",
    "        text = re.sub(r'[-\\s]+', '-', text)\n",
    "        text = text.strip('-')\n",
    "        return text[:50]\n",
    "\n",
    "    def generate_seo_filename(self, studio: Dict) -> str:\n",
    "        try:\n",
    "            name = self.normalize_string(studio.get('title', ''))\n",
    "            neighborhood = self.normalize_string(studio.get('neighborhood', ''))\n",
    "            city = self.normalize_string(studio.get('city_code', ''))\n",
    "            \n",
    "            if not name: name = 'studio'\n",
    "            if not neighborhood: neighborhood = 'centro'\n",
    "            if not city: city = 'sp'\n",
    "            \n",
    "            filename = f'pilates-{neighborhood}-{city}-{name}'\n",
    "            return filename[:100]\n",
    "        except Exception:\n",
    "            return f'pilates-studio-{studio.get(\"id\", \"unknown\")}'\n",
    "\n",
    "    def get_file_extension(self, url: str, content_type: str = '') -> str:\n",
    "        parsed = urlparse(url)\n",
    "        path = Path(parsed.path)\n",
    "        ext = path.suffix.lower()\n",
    "        \n",
    "        if ext in ALLOWED_EXTENSIONS: return ext\n",
    "        if 'jpeg' in content_type or 'jpg' in content_type: return '.jpg'\n",
    "        elif 'png' in content_type: return '.png'\n",
    "        elif 'webp' in content_type: return '.webp'\n",
    "        return '.jpg'\n",
    "\n",
    "    def download_image(self, url: str, filepath: Path) -> bool:\n",
    "        for attempt in range(MAX_RETRIES):\n",
    "            try:\n",
    "                response = self.session.get(url, timeout=TIMEOUT, stream=True)\n",
    "                if response.status_code != 200: continue\n",
    "                \n",
    "                content_type = response.headers.get('content-type', '')\n",
    "                if not any(img_type in content_type.lower() for img_type in ['image/', 'jpeg', 'jpg', 'png', 'webp']):\n",
    "                    continue\n",
    "                \n",
    "                content = b''\n",
    "                for chunk in response.iter_content(chunk_size=8192):\n",
    "                    if chunk:\n",
    "                        content += chunk\n",
    "                        if len(content) > MAX_FILE_SIZE:\n",
    "                            raise ValueError(\"Arquivo muito grande\")\n",
    "                \n",
    "                if len(content) < 1024: continue\n",
    "                \n",
    "                if not self.dry_run:\n",
    "                    with open(filepath, 'wb') as f:\n",
    "                        f.write(content)\n",
    "                    \n",
    "                    try:\n",
    "                        with Image.open(filepath) as img:\n",
    "                            img.verify()\n",
    "                    except Exception:\n",
    "                        filepath.unlink(missing_ok=True)\n",
    "                        continue\n",
    "                \n",
    "                return True\n",
    "            except Exception:\n",
    "                if attempt < MAX_RETRIES - 1:\n",
    "                    time.sleep(2 ** attempt)\n",
    "        return False\n",
    "\n",
    "    def create_thumbnails(self, original_path: Path, base_name: str):\n",
    "        if self.dry_run: return\n",
    "        \n",
    "        try:\n",
    "            with Image.open(original_path) as img:\n",
    "                if img.mode in ('RGBA', 'LA', 'P'):\n",
    "                    img = img.convert('RGB')\n",
    "                \n",
    "                # Thumbnail (300x200)\n",
    "                thumb = img.copy()\n",
    "                thumb.thumbnail((300, 200), Image.Resampling.LANCZOS)\n",
    "                thumb_path = UPLOADS_DIR / 'thumbnails' / f'{base_name}.webp'\n",
    "                thumb.save(thumb_path, 'WEBP', quality=85, optimize=True)\n",
    "                \n",
    "                # Medium (600x400)\n",
    "                medium = img.copy()\n",
    "                medium.thumbnail((600, 400), Image.Resampling.LANCZOS)\n",
    "                medium_path = UPLOADS_DIR / 'medium' / f'{base_name}.webp'\n",
    "                medium.save(medium_path, 'WEBP', quality=90, optimize=True)\n",
    "        except Exception as e:\n",
    "            print(f\"‚ùå Erro ao criar thumbnails: {e}\")\n",
    "\n",
    "    def get_studios_with_images(self, limit: Optional[int] = None) -> List[Dict]:\n",
    "        try:\n",
    "            query = self.supabase.table('studios').select('*').not_.is_('image_url', 'null').neq('image_url', '')\n",
    "            if limit: query = query.limit(limit)\n",
    "            result = query.execute()\n",
    "            return result.data\n",
    "        except Exception as e:\n",
    "            print(f\"‚ùå Erro ao buscar studios: {e}\")\n",
    "            return []\n",
    "\n",
    "    def process_studio(self, studio: Dict) -> bool:\n",
    "        studio_id = studio.get('id')\n",
    "        current_image_url = studio.get('image_url', '')\n",
    "        \n",
    "        if not current_image_url or current_image_url.startswith('pilates-'):\n",
    "            self.stats['skipped'] += 1\n",
    "            return True\n",
    "        \n",
    "        base_filename = self.generate_seo_filename(studio)\n",
    "        \n",
    "        try:\n",
    "            head_response = self.session.head(current_image_url, timeout=10)\n",
    "            content_type = head_response.headers.get('content-type', '')\n",
    "        except:\n",
    "            content_type = ''\n",
    "        \n",
    "        extension = self.get_file_extension(current_image_url, content_type)\n",
    "        filename = f'{base_filename}{extension}'\n",
    "        filepath = UPLOADS_DIR / 'original' / filename\n",
    "        \n",
    "        if filepath.exists() and not self.dry_run:\n",
    "            self.stats['skipped'] += 1\n",
    "            return True\n",
    "        \n",
    "        studio_name = studio.get('title', 'Unknown')[:30]\n",
    "        print(f\"{'üîç [DRY RUN] ' if self.dry_run else '‚¨áÔ∏è '}Baixando {studio_name} -> {filename[:40]}...\")\n",
    "        \n",
    "        if not self.download_image(current_image_url, filepath):\n",
    "            print(f\"‚ùå Falha: {studio_name}\")\n",
    "            self.stats['errors'] += 1\n",
    "            return False\n",
    "        \n",
    "        self.create_thumbnails(filepath, base_filename)\n",
    "        self.stats['downloaded'] += 1\n",
    "        print(f\"‚úÖ Sucesso: {filename[:40]}\")\n",
    "        return True\n",
    "\n",
    "    def run(self, limit: Optional[int] = None):\n",
    "        print(\"üöÄ === Iniciando download de imagens dos studios ===\")\n",
    "        print(f\"üìä Modo: {'DRY RUN (SEGURO)' if self.dry_run else 'PRODU√á√ÉO (VAI BAIXAR!)'}\")\n",
    "        \n",
    "        studios = self.get_studios_with_images(limit)\n",
    "        if not studios:\n",
    "            print(\"‚ö†Ô∏è Nenhum studio encontrado!\")\n",
    "            return\n",
    "        \n",
    "        self.stats['total'] = len(studios)\n",
    "        print(f\"üìã Encontrados {len(studios)} studios para processar\\n\")\n",
    "        \n",
    "        progress = tqdm(total=len(studios), desc=\"Processando studios\")\n",
    "        \n",
    "        for i in range(0, len(studios), BATCH_SIZE):\n",
    "            batch = studios[i:i + BATCH_SIZE]\n",
    "            for studio in batch:\n",
    "                self.process_studio(studio)\n",
    "                progress.update(1)\n",
    "                time.sleep(0.1)\n",
    "        \n",
    "        progress.close()\n",
    "        self.print_summary()\n",
    "\n",
    "    def print_summary(self):\n",
    "        print(\"\\n\" + \"=\"*60)\n",
    "        print(\"üìä RESUMO DA EXECU√á√ÉO\")\n",
    "        print(\"=\"*60)\n",
    "        print(f\"üìà Total de studios: {self.stats['total']}\")\n",
    "        print(f\"‚¨áÔ∏è Imagens baixadas: {self.stats['downloaded']}\")\n",
    "        print(f\"‚è≠Ô∏è Skipped: {self.stats['skipped']}\")\n",
    "        print(f\"‚ùå Erros: {self.stats['errors']}\")\n",
    "        print(\"=\"*60)\n",
    "        \n",
    "        if self.stats['downloaded'] > 0:\n",
    "            print(f\"üìÅ Imagens salvas em: {UPLOADS_DIR}\")\n",
    "            print(\"üñºÔ∏è Thumbnails criados automaticamente\")\n",
    "            print(\"üíæ URLs ser√£o geradas dinamicamente pelo sistema\")\n",
    "        \n",
    "        print(\"\\nüéâ Processamento conclu√≠do!\")\n",
    "\n",
    "print(\"üèóÔ∏è Classe StudioImageDownloader definida!\")"
   ]
